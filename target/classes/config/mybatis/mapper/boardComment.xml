<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="comment">
	<select id="selectList" resultType="boardComment" parameterType="int">
		select * from board_comment where board_idx=#{board_idx} order by seq asc
	</select>
</mapper>












